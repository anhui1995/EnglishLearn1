<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="xin.xiaoa.englishlearn">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".service.ELApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launche1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.AddWordActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.TestActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.WordDetailActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.SearchActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.WordDictationActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".BottomNavigationBarActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.ArticleActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activity.A2WActivity"
            android:label="@string/app_name" />
        <activity
        android:name=".activity.LoginActivity"
        android:label="@string/app_name" />

    </application>

</manifest>